  },
  "devDependencies": {
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "rimraf": "^6.0.1",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joshuademarco/payload-gifs.git"
  },
  "homepage": "https://github.com/joshuademarco/payload-gifs#readme",
  "bugs": {
    "url": "https://github.com/joshuademarco/payload-gifs/issues"
